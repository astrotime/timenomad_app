---
layout: null
tip_DateFormatter: "http://strftime.net"
---

{
	"items": [
		{% for item in site.appfeed %}
		{
			"uuid": {{ item.uuid }},
			"date": "{{ item.date | date: "%Y-%m-%dT%H:%M:%S%z" }}",
			"categories": "{{ item.categories}}",
			"url": "{{ item.url | prepend: site.url }}",
			"title": "{{ item.title }}",
			"blurb": "{{ item.blurb }}"
		},
		{% endfor %}
	]
}